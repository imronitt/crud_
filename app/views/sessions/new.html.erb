<h1>Sessions#new</h1>

<% provide(:title, 'Log in') %>

<h1>Login</h1>
<div>
<%= form_with(url:login_path, scope: :session ,data: { turbo: false }) do |f| %>

<%= f.label :email %><%= f.email_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>


<%= f.submit "Log in" %>

<% end %>
<p>New user? <%= link_to "Sign Up", signup_path %> </p>


</div>

